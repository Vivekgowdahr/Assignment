package collection;

public class MainList {

	Object [] array;
	int position;
	
	public void MainList(int size) {
		array=new Object[size];
	}
	
	public void add(Object obj) {
		if (position<= array.length) {
			increaseCapacity();
		}
		array[position]=obj;
		position++;
	}

	private void increaseCapacity() {
		Object [] temp=new Object[(array.length*3)/2];
		
	}
		
		
	}